<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity" android:orientation="vertical">
    <TextView
            android:text="Hola amigo como te llamas?"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView5"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="24dp"
            android:layout_marginStart="24dp"
            android:layout_alignParentLeft="true" android:layout_marginTop="106dp"
            android:layout_alignParentTop="true" android:textAppearance="@style/TextAppearance.AppCompat.Medium"
            android:textColor="#0D941F"/>
    <EditText
            android:layout_width="201dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Name"
            android:ems="10"
            android:layout_alignParentStart="true" android:layout_marginStart="21dp"
            android:layout_alignParentLeft="true" android:layout_alignParentTop="true" android:id="@+id/editText6"
            android:layout_marginTop="135dp" android:layout_marginLeft="21dp"/>
    <Button
            android:text="Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/button3" android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" android:layout_marginEnd="34dp"
            android:layout_alignTop="@+id/editText6" android:layout_marginRight="34dp" android:layout_marginTop="0dp"/>
    <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@android:drawable/sym_def_app_icon"
            android:layout_alignParentStart="true" android:layout_marginStart="24dp"
            android:layout_alignParentLeft="true" android:layout_alignParentTop="true" android:id="@+id/imageView2"
            android:layout_marginTop="36dp" android:layout_marginLeft="24dp"/>
</RelativeLayout>
